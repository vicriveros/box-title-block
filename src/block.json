{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/box-title",
	"version": "0.1.0",
	"title": "Box Title",
	"category": "widgets",
	"icon": "button",
	"description": "Style your titles with a colored background or border.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
